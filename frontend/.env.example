# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_VERSION=/api/v1

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000/ws

# Feature Flags
VITE_ENABLE_TRADING=true
VITE_ENABLE_ML=true
VITE_ENABLE_ALERTS=true
VITE_ENABLE_BACKUP=true

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_DEFAULT_LANGUAGE=en

# Development
VITE_DEBUG=false
VITE_MOCK_API=false

# External Services
VITE_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key
VITE_YAHOO_FINANCE_API_KEY=your_yahoo_finance_key

# Trading Configuration
VITE_DEFAULT_RISK_LEVEL=medium
VITE_MAX_POSITION_SIZE=10000
VITE_DEFAULT_STOP_LOSS=0.02

# ML Configuration
VITE_ML_MODEL_VERSION=1.0.0
VITE_PREDICTION_CONFIDENCE_THRESHOLD=0.7

# Notifications
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_ENABLE_EMAIL_NOTIFICATIONS=false

# Performance
VITE_CACHE_DURATION=300000
VITE_REQUEST_TIMEOUT=30000
VITE_MAX_RETRIES=3
